<h1>PhotoList</h1>


<% if nil != @photos then %>
  <table>
  <% @photos.each do |photo| %>
    <tr>
      <td>
      <%= image_tag photo.image_url.to_s %>
      </td>
      <td>
  	  <b>Title:</b><%= photo.title %><br>
	  <b>name :</b><%= Person.find(photo.person_id).name %><br>
      </td>
      <td>
    	 <td><%= link_to 'Show', photo %></td>
      </td>
    </tr>
  </table>
  <% end %>
<% end %>
<%= link_to 'Back', photos_path %>
